<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.woo.cookBlog.mapper.PagingMapper">
	<select id="getNumOfBoard" resultType="Integer" parameterType="Integer">
		select count(*) from board LIMIT #{start},#{pageSize}
	</select>
	
	
</mapper>